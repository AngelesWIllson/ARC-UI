{ "namespace": "aui-config_section",
  "root_section": { 
    "type": "stack_panel",
    "size": [ "95%", "100%c" ],
    "anchor_from": "top_middle",
    "anchor_to": "top_middle",
    "controls": [
      { "padding00": { "type": "panel", "size": [ 1, 20 ]}},
      { "label01@aui-common.label_element": { "text": "arc-ui.settingtitle.changelog.config" }},
      { "label02@aui-common.label_element": { "text": "('§7- Config name : §f' + $configname)" }},
      { "label03@aui-common.label_element": { "text": "('§7- Config key : §f' + $key)" }},
      { "label04@aui-common.label_element": { "text": "('§7- Config ver : §f' + $ver)" }},
      { "label06@aui-common.label_element": {
          "text": "$valuetext",
          "$valuetext|default": "§cINVAILD VALUE",
          "$auivar:hover_text": true,
          "$auivar:label_text": "arc-ui.about.config.show_changelog",
          "variables": [
            { "requires": "($show_changelog)", "$valuetext": "('§a█§7 show_changelog : ' + '§aTrue')" },
            { "requires": "(not $show_changelog)", "$valuetext": "('§c█§7 show_changelog : ' + '§cFalse')" }
      ]}},
      { "label12@aui-common.label_element": {
          "text": "$valuetext",
          "$valuetext|default": "§cINVAILD VALUE",
          "$auivar:hover_text": true,
          "$auivar:label_text": "arc-ui.about.config.show_warning_notification",
          "variables": [
            { "requires": "($show_warning_notification)", "$valuetext": "('§a█§7 show_warning_notification : ' + '§aTrue')" },
            { "requires": "(not $show_warning_notification)", "$valuetext": "('§c█§7 show_warning_notification : ' + '§cFalse')" }
      ]}},
      { "label13@aui-common.label_element": {
          "text": "$valuetext",
          "$valuetext|default": "§cINVAILD VALUE",
          "$auivar:hover_text": true,
          "$auivar:label_text": "arc-ui.about.config.no_marketplace_button",
          "variables": [
            { "requires": "($no_marketplace_button)", "$valuetext": "('§a█§7 no_marketplace_button : ' + '§aTrue')" },
            { "requires": "(not $no_marketplace_button)", "$valuetext": "('§c█§7 no_marketplace_button : ' + '§cFalse')" }
      ]}},
      { "label14@aui-common.label_element": {
          "text": "$valuetext",
          "$valuetext|default": "§cINVAILD VALUE",
          "$auivar:hover_text": true,
          "$auivar:label_text": "arc-ui.about.config.legacy_ui_layout",
          "variables": [
            { "requires": "($legacy_ui_layout)", "$valuetext": "('§a█§7 legacy_ui_layout : ' + '§aTrue')" },
            { "requires": "(not $legacy_ui_layout)", "$valuetext": "('§c█§7 legacy_ui_layout : ' + '§cFalse')" }
      ]}},
      { "label15@aui-common.label_element": {
          "text": "$valuetext",
          "$valuetext|default": "§cINVAILD VALUE",
          "$auivar:hover_text": true,
          "$auivar:label_text": "arc-ui.about.config.main_menu_background",
          "variables": [
            { "requires": "($main_menu_background)", "$valuetext": "('§a█§7 main_menu_background : ' + '§aTrue')" },
            { "requires": "(not $main_menu_background)", "$valuetext": "('§c█§7 main_menu_background : ' + '§cFalse')" }
      ]}},
      { "label15@aui-common.label_element": {
          "text": "$valuetext",
          "$valuetext|default": "§cINVAILD VALUE",
          "$auivar:hover_text": true,
          "$auivar:label_text": "arc-ui.about.config.background_renderer",
          "variables": [
            { "requires": "($background_renderer)", "$valuetext": "('§a█§7 background_renderer : ' + '§aTrue')" },
            { "requires": "(not $background_renderer)", "$valuetext": "('§c█§7 background_renderer : ' + '§cFalse')" }
      ]}},
      { "label15@aui-common.label_element": {
          "text": "$valuetext",
          "$valuetext|default": "§cINVAILD VALUE",
          "$auivar:hover_text": true,
          "$auivar:label_text": "arc-ui.about.config.gradient_renderer",
          "variables": [
            { "requires": "($gradient_renderer)", "$valuetext": "('§a█§7 gradient_renderer : ' + '§aTrue')" },
            { "requires": "(not $gradient_renderer)", "$valuetext": "('§c█§7 gradient_renderer : ' + '§cFalse')" }
      ]}},
      { "label15@aui-common.label_element": {
          "text": "$valuetext",
          "$valuetext|default": "§cINVAILD VALUE",
          "$auivar:hover_text": true,
          "$auivar:label_text": "arc-ui.about.config.oled_mode",
          "variables": [
            { "requires": "($oled_mode)", "$valuetext": "('§a█§7 oled_mode : ' + '§aTrue')" },
            { "requires": "(not $oled_mode)", "$valuetext": "('§c█§7 oled_mode : ' + '§cFalse')" }
      ]}},
      { "label15@aui-common.label_element": {
          "text": "$valuetext",
          "$valuetext|default": "§cINVAILD VALUE",
          "$auivar:hover_text": true,
          "$auivar:label_text": "arc-ui.about.config.allow_bottom_bar_customization",
          "variables": [
            { "requires": "($allow_bottom_bar_customization)", "$valuetext": "('§a█§7 allow_bottom_bar_customization : ' + '§aTrue')" },
            { "requires": "(not $allow_bottom_bar_customization)", "$valuetext": "('§c█§7 allow_bottom_bar_customization : ' + '§cFalse')" }
      ]}},
      { "label15@aui-common.label_element": {
          "text": "$valuetext",
          "$valuetext|default": "§cINVAILD VALUE",
          "$auivar:hover_text": true,
          "$auivar:label_text": "arc-ui.about.config.custom_loading_background_overworld",
          "variables": [
            { "requires": "($custom_loading_background_overworld)", "$valuetext": "('§a█§7 custom_loading_background_overworld : ' + '§aTrue')" },
            { "requires": "(not $custom_loading_background_overworld)", "$valuetext": "('§c█§7 custom_loading_background_overworld : ' + '§cFalse')" }
      ]}},
      { "label15@aui-common.label_element": {
          "text": "$valuetext",
          "$valuetext|default": "§cINVAILD VALUE",
          "$auivar:hover_text": true,
          "$auivar:label_text": "arc-ui.about.config.custom_loading_background_nether",
          "variables": [
            { "requires": "($custom_loading_background_nether)", "$valuetext": "('§a█§7 custom_loading_background_nether : ' + '§aTrue')" },
            { "requires": "(not $custom_loading_background_nether)", "$valuetext": "('§c█§7 custom_loading_background_nether : ' + '§cFalse')" }
      ]}},
      { "label15@aui-common.label_element": {
          "text": "$valuetext",
          "$valuetext|default": "§cINVAILD VALUE",
          "$auivar:hover_text": true,
          "$auivar:label_text": "arc-ui.about.config.custom_loading_background_theend",
          "variables": [
            { "requires": "($custom_loading_background_theend)", "$valuetext": "('§a█§7 custom_loading_background_theend : ' + '§aTrue')" },
            { "requires": "(not $custom_loading_background_theend)", "$valuetext": "('§c█§7 custom_loading_background_theend : ' + '§cFalse')" }
      ]}},
      { "label07@aui-common.label_element": { 
        "text": "('§7- Status bar font-type : §f' + $bottom_bar_font_type)"
      }},
      { "labelstack01": {
          "type": "stack_panel",
          "orientation": "horizontal",
          "size": [ "100%", 10 ],
          "controls": [
            { "label01": {
                "type": "label",
                "text": "█ ",
                "alpha": "$bottom_bar_background_alpha"
            }},
            { "label02": {
                "type": "label",
                "text": "('§7< Status bar alpha (' + $bottom_bar_background_alpha + ')')"
          }}
      ]}},
      { "labelstack02": {
          "type": "stack_panel",
          "orientation": "horizontal",
          "size": [ "100%", 10 ],
          "controls": [
            { "label01": {
                "type": "label",
                "text": "█ ",
                "color": "$bottom_bar_background_color"
            }},
            { "label02": {
                "type": "label",
                "text": "§7< Status bar color"
          }}
      ]}},
      { "labelstack03": {
          "type": "stack_panel",
          "orientation": "horizontal",
          "size": [ "100%", 10 ],
          "controls": [
            { "label01": {
                "type": "label",
                "text": "█ ",
                "color": "$bottom_bar_font_color"
            }},
            { "label02": {
                "type": "label",
                "text": "§7< Status bar color [Label/Font]"
          }}
      ]}},
      { "labelstack04": { "type": "stack_panel", "orientation": "horizontal", "size": [ "100%", 10 ], "controls": [{ "label01": { "type": "label", "text": "█ ", "color": "$primary_color" }},{ "label02": { "type": "label", "text": "§7< Primary color" }}]}},
      { "labelstack05": { "type": "stack_panel", "orientation": "horizontal", "size": [ "100%", 10 ], "controls": [{ "label01": { "type": "label", "text": "█ ", "color": "$setting_button_icon_color" }},{ "label02": { "type": "label", "text": "§7< Settings icons color" }}]}},
      { "labelstack06": { "type": "stack_panel", "orientation": "horizontal", "size": [ "100%", 10 ], "controls": [{ "label01": { "type": "label", "text": "█ ", "color": "$slider_icon_color" }},{ "label02": { "type": "label", "text": "§7< Slider icons color" }}]}},
      { "labelstack07": { "type": "stack_panel", "orientation": "horizontal", "size": [ "100%", 10 ], "controls": [{ "label01": { "type": "label", "text": "█ ", "color": "$slider_color" }},{ "label02": { "type": "label", "text": "§7< Slider color" }}]}},
      { "labelstack08": { "type": "stack_panel", "orientation": "horizontal", "size": [ "100%", 10 ], "controls": [{ "label01": { "type": "label", "text": "█ ", "color": "$slider_hover_color" }},{ "label02": { "type": "label", "text": "§7< Slider hover color" }}]}},
      { "labelstack09": { "type": "stack_panel", "orientation": "horizontal", "size": [ "100%", 10 ], "controls": [{ "label01": { "type": "label", "text": "█ ", "color": "$slider_background_color" }},{ "label02": { "type": "label", "text": "§7< Slider background color" }}]}},
      { "labelstack10": { "type": "stack_panel", "orientation": "horizontal", "size": [ "100%", 10 ], "controls": [{ "label01": { "type": "label", "text": "█ ", "color": "$slider_background_hover_color" }},{ "label02": { "type": "label", "text": "§7< Slider background hover color" }}]}},
      { "labelstack11": { "type": "stack_panel", "orientation": "horizontal", "size": [ "100%", 10 ], "controls": [{ "label01": { "type": "label", "text": "█ ", "color": "$background_menu_color" }},{ "label02": { "type": "label", "text": "§7< Main background color" }}]}},
      { "labelstack12": { "type": "stack_panel", "orientation": "horizontal", "size": [ "100%", 10 ], "controls": [{ "label01": { "type": "label", "text": "█ ", "color": "$background_standard_color_top" }},{ "label02": { "type": "label", "text": "§7< Standard background color (Top)" }}]}},
      { "labelstack13": { "type": "stack_panel", "orientation": "horizontal", "size": [ "100%", 10 ], "controls": [{ "label01": { "type": "label", "text": "█ ", "color": "$background_standard_color_bottom" }},{ "label02": { "type": "label", "text": "§7< Standard background color (Bottom)" }}]}},
      { "labelstack14": { "type": "stack_panel", "orientation": "horizontal", "size": [ "100%", 10 ], "controls": [{ "label01": { "type": "label", "text": "█ ", "color": "$toggle_icon_color" }},{ "label02": { "type": "label", "text": "§7< Option/toggle icon color" }}]}},
      { "labelstack15": { "type": "stack_panel", "orientation": "horizontal", "size": [ "100%", 10 ], "controls": [{ "label01": { "type": "label", "text": "█ ", "color": "$toggle_background_color" }},{ "label02": { "type": "label", "text": "§7< Option/toggle hover background color" }}]}},
      { "padding01": { "type": "panel", "size": [ 1, 10 ]}}
]}}
  