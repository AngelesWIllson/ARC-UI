{ "namespace": "aui-setting_sound",
  "empty_element": {
    "type": "stack_panel",
    "size": [ "95%", "100%c" ],
	  "anchor_from": "top_middle",
    "anchor_to": "top_middle",
    "controls": [
      { "pudding00": { "type": "panel", "size": [ 1, 20 ]}}
  ]},
  "main_vol@aui-setting_screen.option_slider": {
    "$option_label": "#main_volume_slider_label",
    "$slider_name": "main_volume",
    "$slider_value_binding_name": "#main_volume",
    "$slider_tts_text_value": "#main_volume_text_value",
    "$option_enabled_binding_name": "#main_volume_enabled",
    "$focus_override_up": "FOCUS_OVERRIDE_STOP",
    "$label_bindings": [{ "binding_name": "$option_label" }]
  },
  "music_vol@aui-setting_screen.option_slider": {
    "$option_label": "#music_volume_slider_label",
    "$slider_name": "music_volume",
    "$slider_value_binding_name": "#music_volume",
    "$slider_tts_text_value": "#music_volume_text_value",
    "$option_enabled_binding_name": "#music_volume_enabled",
    "$label_bindings": [{ "binding_name": "$option_label" }]
  },
  "sound_vol@aui-setting_screen.option_slider": {
    "$option_label": "#sound_volume_slider_label",
    "$slider_name": "sound_volume",
    "$slider_value_binding_name": "#sound_volume",
    "$slider_tts_text_value": "#sound_volume_text_value",
    "$option_enabled_binding_name": "#sound_volume_enabled",
    "$label_bindings": [{ "binding_name": "$option_label" }]
  },
  "ambient_vol@aui-setting_screen.option_slider": {
    "$option_label": "#ambient_volume_slider_label",
    "$slider_name": "ambient_volume",
    "$slider_value_binding_name": "#ambient_volume",
    "$slider_tts_text_value": "#ambient_volume_text_value",
    "$option_enabled_binding_name": "#ambient_volume_enabled",
    "$label_bindings": [{ "binding_name": "$option_label" }]
  },
  "block_vol@aui-setting_screen.option_slider": {
    "$option_label": "#block_volume_slider_label",
    "$slider_name": "block_volume",
    "$slider_value_binding_name": "#block_volume",
    "$slider_tts_text_value": "#block_volume_text_value",
    "$option_enabled_binding_name": "#block_volume_enabled",
    "$label_bindings": [{ "binding_name": "$option_label" }]
  },
  "hostile_vol@aui-setting_screen.option_slider": {
    "$option_label": "#hostile_volume_slider_label",
    "$slider_name": "hostile_volume",
    "$slider_value_binding_name": "#hostile_volume",
    "$slider_tts_text_value": "#hostile_volume_text_value",
    "$option_enabled_binding_name": "#hostile_volume_enabled",
    "$label_bindings": [{ "binding_name": "$option_label" }]
  },
  "neutral_vol@aui-setting_screen.option_slider": {
    "$option_label": "#neutral_volume_slider_label",
    "$slider_name": "neutral_volume",
    "$slider_value_binding_name": "#neutral_volume",
    "$slider_tts_text_value": "#neutral_volume_text_value",
    "$option_enabled_binding_name": "#neutral_volume_enabled",
    "$label_bindings": [{ "binding_name": "$option_label" }]
  },
  "player_vol@aui-setting_screen.option_slider": {
    "$option_label": "#player_volume_slider_label",
    "$slider_name": "player_volume",
    "$slider_value_binding_name": "#player_volume",
    "$slider_tts_text_value": "#player_volume_text_value",
    "$option_enabled_binding_name": "#player_volume_enabled",
    "$label_bindings": [{ "binding_name": "$option_label" }]
  },
  "record_vol@aui-setting_screen.option_slider": {
    "$option_label": "#record_volume_slider_label",
    "$slider_name": "record_volume",
    "$slider_value_binding_name": "#record_volume",
    "$slider_tts_text_value": "#record_volume_text_value",
    "$option_enabled_binding_name": "#record_volume_enabled",
    "$label_bindings": [{ "binding_name": "$option_label" }]
  },
  "weather_vol@aui-setting_screen.option_slider": {
    "$option_label": "#weather_volume_slider_label",
    "$slider_name": "weather_volume",
    "$slider_value_binding_name": "#weather_volume",
    "$slider_tts_text_value": "#weather_volume_text_value",
    "$option_enabled_binding_name": "#weather_volume_enabled",
    "$label_bindings": [{ "binding_name": "$option_label" }]
  },
  "tts_vol@aui-setting_screen.option_slider": {
    "$option_label": "#texttospeech_volume_slider_label",
    "$slider_name": "texttospeech_volume",
    "$slider_value_binding_name": "#texttospeech_volume",
    "$slider_tts_text_value": "#texttospeech_volume_text_value",
    "$option_enabled_binding_name": "#texttospeech_volume_enabled",
    "$label_bindings": [{ "binding_name": "$option_label" }]
  },
  "root_section": {
    "type": "stack_panel",
    "size": [ "100%", "100%c" ],
    "anchor_from": "top_left",
    "anchor_to": "top_left",
    "bindings": [
      { "binding_type": "view",
        "source_control_name": "sound_button_toggle",
        "source_property_name": "#toggle_state",
        "target_property_name": "#visible"
    }],
    "controls": [
      { "pudding00": { "type": "panel", "size": [ "100%", 2 ]}},
      { "main_element@aui-common.header_background_element": { "$auivar-header_contents": "aui-setting_sound.empty_element", "$text": "arc-ui.settingtitle.sound-main" }},
      { "main_vol@aui-common.header_background_slider": { "$auivar-header_contents": "aui-setting_sound.main_vol", "$auivar-header_image": ".aui/.aui-assets/icons/glyph/general_glyph_color", "$auivar-header_text": "arc-ui.setting.sound.main" }},
      { "music_vol@aui-common.header_background_slider": { "$auivar-header_contents": "aui-setting_sound.music_vol", "$auivar-header_image": ".aui/.aui-assets/icons/glyph/music_sound_glyph", "$auivar-header_text": "arc-ui.setting.sound.music" }},
      { "sound_vol@aui-common.header_background_slider": { "$auivar-header_contents": "aui-setting_sound.sound_vol", "$auivar-header_image": ".aui/.aui-assets/icons/glyph/main_sound_glyph", "$auivar-header_text": "arc-ui.setting.sound.sound" }},
      { "pudding01": { "type": "panel", "size": [ "100%", 2 ]}},
      { "sound_element@aui-common.header_background_element": { "$auivar-header_contents": "aui-setting_sound.empty_element", "$text": "arc-ui.settingtitle.sound" }},
      { "ambient_vol@aui-common.header_background_slider": { "$auivar-header_contents": "aui-setting_sound.ambient_vol", "$auivar-header_image": ".aui/.aui-assets/icons/glyph/ambient_sound_glyph", "$auivar-header_text": "arc-ui.setting.sound.ambient" }},
      { "block_vol@aui-common.header_background_slider": { "$auivar-header_contents": "aui-setting_sound.block_vol", "$auivar-header_image": ".aui/.aui-assets/icons/glyph/block_sound_glyph", "$auivar-header_text": "arc-ui.setting.sound.block" }},
      { "hostile_vol@aui-common.header_background_slider": { "$auivar-header_contents": "aui-setting_sound.hostile_vol", "$auivar-header_image": ".aui/.aui-assets/icons/glyph/hostile_sound_glyph", "$auivar-header_text": "arc-ui.setting.sound.hostile" }},
      { "neutral_vol@aui-common.header_background_slider": { "$auivar-header_contents": "aui-setting_sound.neutral_vol", "$auivar-header_image": ".aui/.aui-assets/icons/glyph/neutral_sound_glyph", "$auivar-header_text": "arc-ui.setting.sound.neutral" }},
      { "player_vol@aui-common.header_background_slider": { "$auivar-header_contents": "aui-setting_sound.player_vol", "$auivar-header_image": ".aui/.aui-assets/icons/glyph/multiplayer_glyph_color", "$auivar-header_text": "arc-ui.setting.sound.player" }},
      { "record_vol@aui-common.header_background_slider": { "$auivar-header_contents": "aui-setting_sound.record_vol", "$auivar-header_image": ".aui/.aui-assets/icons/glyph/sound_glyph_color", "$auivar-header_text": "arc-ui.setting.sound.record" }},
      { "weather_vol@aui-common.header_background_slider": { "$auivar-header_contents": "aui-setting_sound.weather_vol", "$auivar-header_image": ".aui/.aui-assets/icons/glyph/weather_sound_glyph", "$auivar-header_text": "arc-ui.setting.sound.weather" }},
      { "tts_vol@aui-common.header_background_slider": { "$auivar-header_contents": "aui-setting_sound.tts_vol", "$auivar-header_image": ".aui/.aui-assets/icons/glyph/accessibility_glyph_color", "$auivar-header_text": "arc-ui.setting.sound.tts" }},
      { "pudding02": { "type": "panel", "size": [ "100%", 2 ]}},
      { "reset_button@aui-setting_screen.action_button": {
          "ignored": "(not $is_pregame)",
          "$pressed_button_name": "button.reset_settings",
          "$button_text": "options.resetSettings",
          "$focus_override_down": "FOCUS_OVERRIDE_STOP",
          "property_bag": { "reset_group": "audio" }
      }}
]}}